{
  "name": "call-filter-server",
  "version": "1.0.0",
  "description": "Serverless backend for Call Filter Dashboard",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc && webpack",
    "build:tsc": "tsc",
    "build:webpack": "webpack",
    "lint": "eslint .",
    "dev": "npm run build && concurrently \"npm run watch\" \"npm run serve\"",
    "watch": "nodemon",
    "serve": "cross-env IS_LOCAL=true serverless offline --stage prod",
    "deploy": "npm run build && cross-env IS_LOCAL=false serverless deploy --stage prod",
    "remove": "serverless remove --stage prod",
    "logs": "serverless logs -f --stage prod",
    "info": "serverless info --stage prod"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.848.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/aws-lambda": "^8.10.158",
    "@types/node": "^24.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "serverless": "^4.0.0",
    "serverless-offline": "^14.4.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
